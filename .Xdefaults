! special colors
*.foreground:  #d0d0d0
*.background:  #000000
*.cursorColor:  #d0d0d0
! black
*.color0:      #000000
*.color8:      #808080
! red
*.color1:      #ff0000
*.color9:      #ff0000
! green
*.color2:      #33ff00
*.color10:     #33ff00
! yellow
*.color3:      #33ff00
*.color11:     #33ff00
! blue
*.color4:      #5294e2
*.color12:     #5294e2
! magenta
*.color5:      #cc00ff
*.color13:     #cc00ff
! cyan
*.color6:      #5294e2
*.color14:     #5294e2
! white
*.color7:      #d0d0d0
*.color15:     #ffffff

*geometry: 99x32    
*highlightSelection: true
*cursorBlink: off
*boldMode: on
*colorMode: on
*colorBDMode: on
*colorULMode: on
*underLine: on
 
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.iso14755: False
URxvt.perl-ext-common:  default,matcher,url-select,keyboard-select, searchable-scrollback,clipboard,vtwheel
URxvt*cutchars: "()*,<>[]{}|'"
URxvt.urgentOnBell: True
URxvt*termName: rxvt-unicode
URxvt*urlLauncher: /usr/bin/chromium
URxvt*matcher.button: 1
URxvt.scrollBar: False
URxvt*transparent: True
URxvt*shading: 10
URxvt*font: xft:DejaVu Sans Mono:pixelsize=12
URxvt*boldFont: xft:DejaVu Sans Mono:pixelsize=12
URxvt.matcher.button: 1
URxvt.matcher.rend.0: Uline Bold fg5
